			{
				return true;
			}
			
			// Get custom location from PackageRegistry
			var packageRegistryPlistPath = my.target.mountpoint + "/Library/Application Support/GarageBand/Package Registry.plist";
			if( system.files.fileExistsAtPath(packageRegistryPlistPath) )
			{
				var packageRegistry = system.files.plistAtPath(packageRegistryPlistPath);
				var prGarageBand = packageRegistry["Jam Pack World Music"];
				
				if( prGarageBand )
				{
					var prInstrumentLibrary = prGarageBand["Instrument Library"];
					if( prInstrumentLibrary )
					{
						if( system.files.fileExistsAtPath(prInstrumentLibrary + "/" + representativeInstrumentLocation) )
						{
							return true;
						}
					}
				}
			}
	
			return false;
		}
	
