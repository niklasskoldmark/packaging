		function GB6orLater()
		{
			var app = '/Applications/GarageBand.app';
			var appBundle = system.files.bundleAtPath(app);
			
			if( appBundle )
			{
				var appVersion = GetBundleFiveTupleVersion(appBundle);
				
				if( appVersion &amp;&amp; (system.compareVersions(appVersion, "6.0.0.0.0") >= 0) ){ return true; }
			}

			return false;
		}
	
